{
	"manifest_version": 2,
	"name": "Vamp2",
	"version": "1.0",
	"description": "Troque todas as imagens do site por fotos do Vampeta.",
	"icons": { "48": "icon48.png", "128": "icon128.png" },
	"content_scripts": [
		{
			"matches": ["*://*/*"],
			"js": ["vamp.js"]
		}
	],
	"browser_specific_settings": {
		"gecko": {
			"id": "vamp@manse.dev.br"
		}
	}
}

